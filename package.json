{
  "name": "@graphql-codegen/typescript-urql",
  "version": "0.0.1",
  "description": "GraphQL Code Generator plugin for generating a ready-to-use React Components/HOC/Hooks based on GraphQL operations with urql",
  "repository": "git@github.com:berezovyy/graphql-urql-generator.git",
  "license": "MIT",
  "scripts": {
    "build": "tsc -m esnext --outDir dist/esnext && tsc -m commonjs --outDir dist/commonjs"
  },
  "peerDependencies": {
    "graphql-tag": "^2.0.0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.5.0",
    "@graphql-codegen/visitor-plugin-common": "1.5.0",
    "path": "^0.12.7",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.5.0",
    "graphql": "14.4.2",
    "typescript": "3.5.3"
  },
  "sideEffects": false,
  "main": "dist/commonjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
  "typescript": {
    "definition": "dist/esnext/index.d.ts"
  },
  "publishConfig": {
    "access": "public"
  }
}
